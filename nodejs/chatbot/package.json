    "path": "^0.12.7",
    "pkg": "^5.8.1",
    "properties-reader": "2.3.0",
    "signal-exit": "4.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-eslint-plugin": "^6.4.0",
